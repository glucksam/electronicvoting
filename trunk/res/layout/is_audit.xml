<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
android:id="@+id/widget0"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:background="#ff6699ff"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/widget42"
android:layout_width="196px"
android:layout_height="35px"
android:text="IS this an audit ballot?"
android:textSize="16sp"
android:typeface="monospace"
android:textStyle="bold"
android:textColor="@color/black"
android:gravity="center"
android:layout_x="51px"
android:layout_y="62px"
>
</TextView>
<Button
android:id="@+id/widget44"
android:layout_width="78px"
android:layout_height="wrap_content"
android:text="Yes"
android:textSize="16sp"
android:textStyle="bold"
android:gravity="center"
droiddraw:onClickListener="Yes"
android:layout_x="36px"
android:layout_y="157px"
>
</Button>
<Button
android:id="@+id/widget45"
android:layout_width="78px"
android:layout_height="wrap_content"
android:text="No"
android:textSize="16sp"
android:textStyle="bold"
android:gravity="center"
droiddraw:onClickListener="No"
android:layout_x="178px"
android:layout_y="157px"
>
</Button>
</AbsoluteLayout>
